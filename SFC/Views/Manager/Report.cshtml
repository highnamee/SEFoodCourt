
@{
    ViewBag.Title = "Report";
    Layout = "~/Views/Shared/_M_Layout_Logined.cshtml";
}
<script>
   
</script>
<main>
    <div class="container-fluid">
        @*<h1 class="mt-4">Reports</h1>*@
        <ol class="breadcrumb mb-4" style="margin-top: 10px">
            <li class="breadcrumb-item"><a href="~/Manager/DashBoard">Dashboard</a></li>
            <li class="breadcrumb-item active">Reports</li>
        </ol>

        <div id="get-report">
            <div class="container">
                <div id="get-report-row" class="row justify-content-center align-items-center">
                    <div id="get-report-column" class="col-md-6">
                        <div id="get-report-box" class="col-md-12">
                            <form id="get-report-form" class="form" method="post">
                                <br />
                                <h3 class="text-center text-info text-white">Get Report</h3>

                                <div class="form-group">
                                    <label for="month" class="text-info text-white">Month:</label><br>
                                    <input type="month" name="month" id="month" class="form-control" minlength="8" required>
                                </div>

                                <div class="justify-content-sm-center text-center">
                                    <input type="button" name="get-report" class="btn btn-primary btn-md" onclick="" style="background-color:#016485" value="Get Report">
                                </div>

                                <div class=" text-center ">
                                    <label for="msg" id="msg" style=" border:1px solid #550519; background-color:#b20202; visibility:hidden;" class="text-primary text-white"> abc </label><br>
                                </div>

                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</main>

<script src="https://code.jquery.com/jquery-3.5.1.min.js" crossorigin="anonymous"></script>
